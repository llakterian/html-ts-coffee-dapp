{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["dom", "es2015", "es2016", "es2017"],
    "module": "none",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["*.ts"]
}
